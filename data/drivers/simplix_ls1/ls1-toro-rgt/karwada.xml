<!DOCTYPE params SYSTEM "params.dtd">


<params name="ls1-toro-rgt">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="10" val="7.57"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="25" val="20.45"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-2"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="2.5"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="1.75"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.348"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.091"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="0.919"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="0.832"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" min="0" max="10" val="3.848"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.5" max="0.65" val="0.6487"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="50000" val="33400"/>
    <attnum name="brake corr lr" min="0" max="0.5" val="0.34003"/>
    <attnum name="brake corr fr" min="0" max="0.5" val="0.11174"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="15.21"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="11.84"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="130" val="60"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="-0.2"/>
    <attnum name="camber" unit="deg" min="-5" max="5" val="-0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="130" val="60"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="0.2"/>
    <attnum name="camber" unit="deg" min="-5" max="5" val="-0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="130" val="60"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="5" val="-0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="130" val="60"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="-0"/>
    <attnum name="camber" unit="deg" min="-5" max="5" val="-0"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="160.8"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="12.02"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="23.78"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="4.14"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="7.89"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="160.8"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="12.02"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="23.78"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="4.14"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="7.89"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="136.61"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.14"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="9.86"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="20.11"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="3.1"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="5.86"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="136.61"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.14"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="9.86"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="20.11"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="3.1"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="5.86"/>
  </section>

  <section name="simplix private">
    <attnum name="scale brake" min="0.5" max="2" val="1.47"/>
    <attnum name="scale mu" min="0.5" max="2" val="0.94"/>
    <attnum name="scale brake rain" min="0" max="1" val="1"/>
    <attnum name="scale mu rain" min="0" max="1" val="1"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" unit="m" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="80"/>
    <attnum name="scale bump" min="0.001" max="0.6" val="0.207"/>
    <attnum name="scale bump outer" min="0" max="2" val="0.4"/>
    <attnum name="limit side use" min="0" max="1" val="1"/>
    <attnum name="limit side width" min="0" max="1.5" val="1"/>
    <attnum name="base mode" min="0.97" max="1.03" val="1.01657"/>
    <attnum name="base scale" min="1" max="1.05" val="1.01304"/>
    <attnum name="lookaheadfactor" min="0.01" max="0.04" val="0.019"/>
    <attnum name="accel delta" min="0" max="1" val="0.08"/>
    <attnum name="accel out" min="0" max="1" val="0"/>
    <attnum name="clutch max" min="0.001" max="1" val="0.5"/>
    <attnum name="border inner" min="-0.5" max="1" val="-0.2"/>
    <attnum name="border outer" min="0" max="1" val="0.4"/>
    <attnum name="fly height" min="0.01" max="1" val="0.2"/>
    <attnum name="tcl slip" min="1" max="10" val="1.5"/>
    <attnum name="tcl factor" min="1" max="10" val="1"/>
    <attnum name="drift factor" min="1" max="10" val="2"/>
    <attnum name="scale steer" min="0.1" max="2" val="0.8"/>
    <attnum name="length margin" min="1" max="10" val="3"/>
    <attnum name="avoid width" min="0" max="3" val="0.5"/>
    <attnum name="scale avoid" min="1" max="20" val="15"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="100" val="-1"/>
  </section>

</params>
