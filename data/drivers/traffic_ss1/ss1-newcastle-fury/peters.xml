<!DOCTYPE params SYSTEM "params.dtd">


<params name="ls1-newcastle-fury">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="5" max="9" val="6.04"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="4" max="23" val="22.04"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-2"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="9" val="2.44"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="1.95"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.56"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.25"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="0.8"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" min="1" max="10" val="4.057"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.6" max="0.7" val="0.6289"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="50000" val="35700"/>
    <attnum name="brake corr lr" min="0" max="0.5" val="0.31047"/>
    <attnum name="brake corr fr" min="0" max="0.5" val="0.0983"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="8.84"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="16.76"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="60"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0.1"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="60"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="-0.1"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="60"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
    <attnum name="camber" unit="deg" min="-4" max="0" val="0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="60"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="-0"/>
    <attnum name="camber" unit="deg" min="-4" max="0" val="0"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="127.8"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="11.99"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="24.02"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="3.99"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="7.98"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="127.8"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="11.99"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="24.02"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="3.99"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="7.98"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="143"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.14"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="9.89"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="18.99"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="3.02"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="6.02"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="143"/>
    <attnum name="suspension course" unit="m" min="0" max="0.28" val="0.14"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="9.89"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="18.99"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="3.02"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="6.02"/>
  </section>

  <section name="simplix private">
    <attnum name="scale brake" min="0.5" max="2" val="1.5"/>
    <attnum name="scale mu" min="0.5" max="2" val="0.92"/>
    <attnum name="scale brake rain" min="0" max="1" val="1"/>
    <attnum name="scale mu rain" min="0" max="1" val="1"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" unit="m" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="80"/>
    <attnum name="scale bump" min="0.001" max="0.6" val="0.3"/>
    <attnum name="scale bump outer" min="0" max="2" val="0.4"/>
    <attnum name="limit side use" min="0" max="1" val="1"/>
    <attnum name="limit side width" min="0" max="1.5" val="1"/>
    <attnum name="base mode" min="0.97" max="1.03" val="1.00119"/>
    <attnum name="base scale" min="1" max="1.05" val="1.00487"/>
    <attnum name="lookaheadfactor" min="0.01" max="0.05" val="0.028"/>
    <attnum name="accel delta" min="0" max="1" val="0.08"/>
    <attnum name="accel out" min="0" max="1" val="0"/>
    <attnum name="clutch max" min="0.001" max="1" val="0.5"/>
    <attnum name="border inner" min="-0.5" max="1" val="-0.2"/>
    <attnum name="border outer" min="0" max="1" val="0.4"/>
    <attnum name="fly height" min="0.01" max="1" val="0.2"/>
    <attnum name="jump offset" min="0" max="0.1" val="0.04"/>
    <attnum name="tcl slip" min="1" max="10" val="1"/>
    <attnum name="tcl factor" min="1" max="10" val="1.4"/>
    <attnum name="drift factor" min="1" max="10" val="2"/>
    <attnum name="scale steer" min="0.1" max="2" val="1"/>
    <attnum name="length margin" min="1" max="10" val="3"/>
    <attnum name="avoid width" min="0" max="3" val="0.5"/>
    <attnum name="scale avoid" min="1" max="20" val="15"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="100" val="-1"/>
  </section>

</params>
