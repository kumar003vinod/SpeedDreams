<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ls1-taipan-ltsr" type="template" mode="mw">
      <!-- Car parameters >>> -->
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="7" max="12" val="12.0"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="4" max="21" val="18.0"/>
	</section>

	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-2.0"/>
			</section>

			<section name="1">
				<attnum name="ratio" min="0" max="5" val="3.13"/>
			</section>

			<section name="2">
				<attnum name="ratio" min="0" max="5" val="2.04"/>
			</section>

			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.5"/>
			</section>

			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.21"/>
			</section>

			<section name="5">
				<attnum name="ratio" min="0" max="5" val="0.92"/>
			</section>

			<section name="6">
				<attnum name="ratio" min="0" max="5" val="0.82"/>
			</section>
		</section>
	</section>

	<section name="Rear Differential">
		<attstr name="type" in="LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="ratio" min="1.0" max="10.0" val="3.13"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.5" max="0.7" val="0.65"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="45000"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="0"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="10"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.2"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.2"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="120"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.10"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="13"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="26"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="9"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="120"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.10"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="13"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="26"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="9"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="140"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.10"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="11"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="22"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="7"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="140"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.10"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="11"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="22"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="7"/>
	</section>
      <!-- <<< Car parameters -->

      <!-- Robot parameters >>> -->
	<section name="simplix private">
            <!-- Use this parameters to test the car's pitting -->
		<attnum name="pit test stop" min="0" max="1" val="0"/>
		<attnum name="qualification" min="0" max="1" val="0"/>

            <!-- Use this parameters to setup the car's fuel strategy -->
		<attnum name="reserve" unit="m" min="0.0" max="20000.0" val="2000"/>
		<attnum name="start fuel" min="-1" max="60.0" val="-1"/>
		<attnum name="fuelper100km" min="1.0" max="100.0" val="80.0"/>

            <!-- Use the parameters below to tune the robots detection
                 of changes in the track's height -->
		<attnum name="scale bump" min="0.0" max="2.0" val="0.001"/>
		<attnum name="scale bump outer" min="0.0" max="2.0" val="0.001"/>

            <!-- Use the parameters below to tune the robots 
                 detection of the car's limits -->
		<attnum name="accel out" min="0" max="1" val="1"/>
		<attnum name="fly height" min="0.01" max="1.0" val="0.20"/>
		<attnum name="jump offset" min="0" max="0.1" val="0.04"/>
		<attnum name="tcl slip" min="1.0" max="10.0" val="1.0"/>
		<attnum name="tcl factor" min="1.0" max="10.0" val="1.4"/>
		<attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.05"/>
		<attnum name="scale steer" min="0.1" max="2.0" val="1.0"/>
		<attnum name="brake limit" min="-30" max="-1.0" val="-15.0"/>
		<attnum name="scale brake" min="0" max="2" val="0.75"/>
		<attnum name="scale mu" min="0" max="5" val="0.85"/>
		<attnum name="scale brake rain" min="0" max="1" val="1.00"/>
		<attnum name="scale mu rain" min="0" max="1" val="1.00"/>

            <!-- Use the parameters below to tune the robots 
                 detection of the car's limits while qualification -->
		<attnum name="qualy brake" min="0" max="2" val="0.75"/>
		<attnum name="qualy mu" min="0" max="2" val="0.85"/>

            <!-- Do not change the parameters below 
                 unless you really know what you are doing -->
		<attnum name="length margin" min="1.0" max="10.0" val="3.0"/>
		<attnum name="avoid width" min="0.0" max="3.0" val="0.5"/>
		<attnum name="scale avoid" min="1.0" max="20.0" val="15.0"/>
		<attnum name="scale min mu" min="0.1" max="1.0" val="0.8"/>
		<attnum name="stay together" min="10.0" max="2000.0" val="10"/>

	</section>
      <!-- <<< Robot parameters -->
</params>
