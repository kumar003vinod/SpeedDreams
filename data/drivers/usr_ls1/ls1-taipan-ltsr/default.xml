<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ls1-taipan-ltsr" type="template" mode="mw">

	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="7" max="12" val="12.0"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="4" max="21" val="18.1"/>
	</section>

	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-2.0"/>
			</section>

			<section name="1">
				<attnum name="ratio" min="0" max="5" val="2.66"/>
			</section>

			<section name="2">
				<attnum name="ratio" min="0" max="5" val="1.78"/>
			</section>

			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.348"/>
			</section>

			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.091"/>
			</section>

			<section name="5">
				<attnum name="ratio" min="0" max="5" val="0.919"/>
			</section>

			<section name="6">
				<attnum name="ratio" min="0" max="5" val="0.832"/>
			</section>
		</section>
	</section>

	<section name="Rear Differential">
		<attnum name="ratio" min="1.0" max="10.0" val="3.7"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.60"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="30000"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="17"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="10"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="13"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="26"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="9"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="13"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="26"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="9"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="140"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="11"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="22"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="7"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="140"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="11"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="22"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="7"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="private">
		<attnum name="min corner inverse" min="0" max="2" val="0.0013"/>
		<attnum name="add avoid speed" min="-20" max="20" val="0.0"/>
		<attnum name="avoid speed factor" min="-20" max="20" val="1.00"/>
		<attnum name="avoid brake factor" min="-20" max="20" val="0.95"/>
		<attnum name="exit boost factor" min="-20" max="20" val="1.00"/>
		<attnum name="avoid exit boost factor" min="-20" max="20" val="1.2"/>
		<attnum name="brake margin" min="-20" max="20" val="1.8"/>
		<attnum name="base speed factor" min="0.0" max="50.0" val="1.00"/>
		<attnum name="base brake factor" min="0.0" max="50.0" val="1.00"/>
		<attnum name="accel redux factor" min="-20" max="20" val="2.5"/>
		<attnum name="base speed" min="0.0" max="50.0" val="12.40"/>
		<attnum name="base brake" min="0.0" max="200.0" val="48.00"/>
		<attnum name="brake mod" min="0.0" max="2.0" val="0.30"/>
		<attnum name="steer mod" min="0.0" max="1.0" val="1.00"/>
		<attnum name="slope factor" min="0.0" max="1.0" val="0.15"/>
		<attnum name="accel exit" min="0.0" max="10.0" val="1.40"/>
		<attnum name="turn decel" min="0" max="2" val="0.4"/>
		<attnum name="curve factor" min="0" max="2" val="0.13"/>
		<attnum name="accel curve" min="0" max="2" val="1.30"/>
		<attnum name="brake curve" min="0" max="2" val="1.00"/>
		<attnum name="accel curve limit" min="0" max="6" val="2.5"/>
		<attnum name="brake curve limit" min="0" max="6" val="1.1"/>
		<attnum name="revs change up" min="0" max="2" val="0.83"/>
		<attnum name="revs change down" min="0" max="2" val="0.63"/>
		<attnum name="revs change down max" min="0" max="2" val="0.73"/>
		<attnum name="steer skid" min="0" max="3" val="0.0"/>
		<attnum name="steer gain" min="0" max="3" val="1.2"/>
		<attnum name="max steer time" min="0.5" max="10" val="2.0"/>
		<attnum name="min steer time" min="0.5" max="10" val="1.77"/>
		<attnum name="int margin" min="0" max="4" val="2.0"/>
		<attnum name="ext margin" min="0" max="4" val="1.0"/>
		<attnum name="no team waiting" min="0" max="2" val="1"/>
		<attnum name="inc factor" min="0" max="5" val="1.10"/>
		<attnum name="overtake caution" min="-20" max="20" val="0.1"/>
		<attnum name="abs slip" min="0.5" max="4.5" val="4.0"/>
		<attnum name="abs range" min="0.5" max="10.5" val="9.0"/>
		<attnum name="tcl slip" min="0.5" max="8.5" val="7.0"/>
		<attnum name="tcl range" min="0.5" max="20.0" val="20.0"/>
		<attnum name="accel mod" min="0" max="1" val="1"/>
		<attnum name="skid correction" min="0.0" max="8.5" val="0.4"/>
		<attnum name="bump caution" min="0.0" max="4.0" val="0.4"/>
		<attnum name="oversteer asr" min="-0.0" max="2.0" val="0.2"/>
		<attnum name="side margin" min="0.0" max="4.0" val="1.0"/>
		<attnum name="pit offset" min="-200.0" max="200.0" val="-50"/>
		<attnum name="steer debug" min="-5" max="5" val="-1.0"/>
		<attnum name="overtake debug" min="0" max="1" val="0"/>
		<attnum name="raceline debug" min="0" max="1" val="0"/>
	</section>
</params>
