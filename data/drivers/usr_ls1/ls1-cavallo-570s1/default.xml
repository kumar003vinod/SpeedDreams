<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Viper GTS-R" type="template">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="6" max="10" val="9"/>
	</section>

	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="4" max="22" val="22"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.60"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="26000"/>
	</section>


	<section name="Front Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="100"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.16"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="12"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="24"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="8"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="100"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.16"/>
		<attnum name="bellcrank" val="1.0"/>
