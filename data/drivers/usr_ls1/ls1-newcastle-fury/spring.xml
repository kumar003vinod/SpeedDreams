<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Viper GTS-R" type="template">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="6" max="10" val="10"/>
	</section>

	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="4" max="22" val="21"/>
	</section>

	<section name="private">
		<attnum name="min corner inverse" min="0" max="2" val="0.0010"/>
		<attnum name="int margin" val="1.00"/>
		<attnum name="ext margin" val="1.20"/>
		<attnum name="base brake" min="10.0" max="200.0" val="60.00"/>
		<attnum name="avoid brake factor" min="-20" max="20" val="0.85"/>

		<attnum name="0 bgn" val="860"/>
		<attnum name="0 end" val="900"/>
		<attnum name="0 base speed" val="11.5"/>
		<attnum name="0 int margin" val="2.0"/>

		<attnum name="3 bgn" val="900"/>
		<attnum name="3 end" val="940"/>
		<attnum name="3 ext margin" val="2.0"/>

		<attnum name="4 bgn" val="6690"/>
		<attnum name="4 end" val="6780"/>
		<attnum name="4 base speed" val="12.0"/>

		<attnum name="1 bgn" val="1180"/>
		<attnum name="1 end" val="1220"/>
		<attnum name="1 int margin" val="2.0"/>

		<attnum name="2 bgn" val="8490"/>
		<attnum name="2 end" val="8650"/>
		<attnum name="2 int margin" val="2.0"/>

		<attnum name="5 bgn" val="7890"/>
		<attnum name="5 end" val="7940"/>
		<attnum name="5 int margin" val="2.0"/>
	</section>

</params>














