<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Viper GTS-R" type="template">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="6" max="10" val="10"/>
	</section>

	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="4" max="22" val="22"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="80"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.3"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5.0"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="80"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.3"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5.0"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="80"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0.0"/>
		<attnum name="camber" min="-4" max="0" unit="deg" val="0.0"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="80"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0.0"/>
		<attnum name="camber" min="-4" max="0" unit="deg" val="0.0"/>
	</section>

  <section name="Front Right Suspension">
    <attnum name="spring" min="100" max="410" unit="kN/m" val="100"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" min="100" max="410" unit="kN/m" val="100"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" min="90" max="410" unit="kN/m" val="160"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" min="90" max="410" unit="kN/m" val="160"/>
  </section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="10"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="10"/>
	</section>

	<section name="private">
		<attnum name="min corner inverse" min="0" max="2" val="0.0010"/>
		<attnum name="rough limit" min="0" max="2" val="0.02"/>
		<attnum name="base speed" min="0.0" max="50.0" val="16.60"/>
		<attnum name="ext margin" min="0" max="2" val="0.5"/>
		<attnum name="slope factor" min="0.0" max="1.0" val="0.30"/>

		<attnum name="3 bgn" val="10"/>
		<attnum name="3 end" val="140"/>
		<attnum name="3 ext margin" val="2.5"/>

		<attnum name="2 bgn" val="1000"/>
		<attnum name="2 end" val="1270"/>
		<attnum name="2 base speed" val="13.3"/>

		<attnum name="1 bgn" val="1300"/>
		<attnum name="1 end" val="1380"/>
		<attnum name="1 rl left margin" val="2"/>

		<attnum name="0 bgn" val="1430"/>
		<attnum name="0 end" val="1490"/>
		<attnum name="0 rl left margin" val="4"/>
		<attnum name="0 base speed" val="8"/>

		<attnum name="4 bgn" val="330"/>
		<attnum name="4 end" val="360"/>
		<attnum name="4 base speed" val="13"/>
	</section>

</params>














