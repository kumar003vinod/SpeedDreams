<!DOCTYPE params SYSTEM "params.dtd">


<params name="MPA1-Furia">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="24.0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="24.0"/>
  </section>

  <section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-2"/>
			</section>
			<section name="1">
				<attnum name="ratio" min="0" max="5" val="5.000"/>
			</section>
			<section name="2">
				<attnum name="ratio" min="0" max="5" val="3.054"/>
			</section>
			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.784"/>
			</section>
			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.315"/>
			</section>
			<section name="5">
				<attnum name="ratio" min="0" max="5" val="1.132"/>
			</section>
			<section name="6">
				<attnum name="ratio" min="0" max="5" val="1.000"/>
			</section>
		</section>
  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <!-- ratio for zero    wing angles:  0/0  3.25 (top speed 385 km/h at straight-10) -->
    <!-- ratio for forza   wing angles: 10/3  3.65 (top speed 347 km/h) -->
    <!-- ratio for default wing angles: 24/24 4.30 (top speed 292 km/h) -->
    <attnum name="ratio" min="0" max="10" val="4.3"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.5"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="65000" val="45000"/>
    <attnum name="brake corr lr" min="0" max="0.5" val="0.28"/>
    <attnum name="brake corr fr" min="0" max="0.5" val="0.14"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="50" max="100" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0.1"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-3"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="50" max="100" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="-0.1"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-3"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="50" max="100" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-1"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="50" max="100" val="80"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-1"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="16.0"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="28.0"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="195"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="4.2"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="4.1"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="2.25"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="2.50"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="195"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="4.2"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="4.1"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="2.25"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="2.50"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="145.0"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="3.8"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="4.4"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="2.4"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="2.8"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="100" max="410" val="145.0"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.16"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="70" val="3.8"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="70" val="4.4"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="70" val="2.4"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="70" val="2.8"/>
  </section>

  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="100" val="-1"/>

    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" unit="m" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="100"/>

    <attnum name="scale bump" min="0.001" max="0.6" val="0.46"/>
    <attnum name="scale bump outer" min="0" max="2" val="0.8"/>

    <attnum name="accel out" min="0" max="1" val="1"/>
    <attnum name="tcl slip" min="1" max="10" val="1.0"/>
    <attnum name="drift factor" min="1" max="10" val="2.0"/>

    <attnum name="lookaheadfactor" min="0.01" max="0.05" val="0.04"/>

    <attnum name="base mode" min="0.97" max="1.03" val="0.99"/>
    <attnum name="base scale" min="1" max="1.05" val="1.012"/>

    <attnum name="scale steer" min="0.1" max="2" val="0.6"/>
    <attnum name="scale brake" min="0.5" max="2" val="0.95"/>
    <attnum name="scale mu" min="0.5" max="2" val="0.85"/>

    <attnum name="qualy brake" min="0" max="2" val="0.95"/>
    <attnum name="qualy mu" min="0" max="2" val="0.85"/>

    <attnum name="length margin" min="1" max="10" val="3"/>
    <attnum name="avoid width" min="0" max="3" val="1"/>
    <attnum name="scale avoid" min="1" max="20" val="20"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
  </section>

</params>
