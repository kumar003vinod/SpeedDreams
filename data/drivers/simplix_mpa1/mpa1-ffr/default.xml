<!DOCTYPE params SYSTEM "params.dtd">


<params name="mpa1-ffr">
  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="100" val="-1"/>

    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" unit="m" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="100"/>

    <attnum name="scale bump" min="0.001" max="0.6" val="0.46"/>
    <attnum name="scale bump outer" min="0" max="2" val="0.8"/>

    <attnum name="accel out" min="0" max="1" val="0"/>
    <attnum name="tcl slip" min="1" max="10" val="1.0"/>
    <attnum name="drift factor" min="1" max="10" val="3.0"/>

    <attnum name="lookaheadfactor" min="0.01" max="0.05" val="0.01"/>

    <attnum name="base mode" min="0.97" max="1.03" val="0.99"/>
    <attnum name="base scale" min="1" max="1.05" val="1.012"/>

    <attnum name="scale steer" min="0.1" max="2" val="1.0"/>
    <attnum name="scale brake" min="0.5" max="2" val="0.9"/>
    <attnum name="scale mu" min="0.5" max="2" val="0.9"/>

    <attnum name="qualy brake" min="0" max="2" val="0.9"/>
    <attnum name="qualy mu" min="0" max="2" val="0.9"/>

    <attnum name="length margin" min="1" max="10" val="3"/>
    <attnum name="avoid width" min="0" max="3" val="1"/>
    <attnum name="scale avoid" min="1" max="20" val="20"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
  </section>

</params>
