<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="sc-cavallo-360" type="template" mode="mw">
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.65" val="0.3439"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="50000" val="40600"/>
	</section>
	
	<section name="private">
		<attnum name="fuelperlap" val="1.3"/>
		<attnum name="base speed" val="7.00"/>
		<attnum name="base brake" val="2.00"/>
		<attnum name="revs change up" val="0.80"/>
		<attnum name="revs change down" val="0.70"/>
		<attnum name="int margin" min="0" max="2" val="1.1"/>
		<attnum name="ext margin" min="0" max="2" val="1.5"/>
		<attnum name="exit boost factor" min="-20" max="20" val="1.0"/>
		<attnum name="slope factor" val="0.1"/>
		<attnum name="brake mu" val="0.50"/>
	</section>
</params>














