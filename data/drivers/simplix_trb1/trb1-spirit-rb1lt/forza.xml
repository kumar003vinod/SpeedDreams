<!DOCTYPE params SYSTEM "params.dtd">


<params name="trb1-spirit-rb1lt">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" val="1.5"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" val="5.1"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-2"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="3.5"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.6"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.9"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.54"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1.25"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="1.08"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" val="3.19"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" val="0.3"/>
    <attnum name="max pressure" unit="kPa" val="43100"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" val="530"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" val="2630"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" val="2600"/>
    <attnum name="suspension course" min="0" max="0.25" unit="m" val="0.15"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="20" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" val="400"/>
    <attnum name="slow rebound" unit="lbs/in/s" val="378"/>
    <attnum name="fast bump" unit="lbs/in/s" val="305"/>
    <attnum name="fast rebound" unit="lbs/in/s" val="308"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" val="2600"/>
    <attnum name="suspension course" min="0" max="0.25" unit="m" val="0.15"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="20" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" val="400"/>
    <attnum name="slow rebound" unit="lbs/in/s" val="378"/>
    <attnum name="fast bump" unit="lbs/in/s" val="305"/>
    <attnum name="fast rebound" unit="lbs/in/s" val="308"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" val="1800"/>
    <attnum name="suspension course" min="0" max="0.25" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" val="287"/>
    <attnum name="slow rebound" unit="lbs/in/s" val="322"/>
    <attnum name="fast bump" unit="lbs/in/s" val="203"/>
    <attnum name="fast rebound" unit="lbs/in/s" val="387"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" val="1800"/>
    <attnum name="suspension course" min="0" max="0.25" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" val="287"/>
    <attnum name="slow rebound" unit="lbs/in/s" val="322"/>
    <attnum name="fast bump" unit="lbs/in/s" val="203"/>
    <attnum name="fast rebound" unit="lbs/in/s" val="387"/>
  </section>

  <section name="simplix private">
    <attnum name="skill" min="-1" max="10" val="0"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" val="0"/>
    <attnum name="reserve" min="0" max="20000" val="2000"/>
    <attnum name="start fuel" min="-1" max="100" val="-1"/>
    <attnum name="fuelper100km" min="1" max="100" val="70"/>
    <attnum name="lookaheadfactor" val="0.03"/>
    <attnum name="avoid width" min="0" max="3" val="0.5"/>
    <attnum name="drift factor" min="1" max="10" val="1.5"/>
    <attnum name="scale avoid" min="1" max="20" val="10"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="scale brake rain" min="0" max="2" val="0.9"/>
    <attnum name="scale mu rain" min="0" max="2" val="0.9"/>
    <attnum name="scale steer" min="0.1" max="2" val="1"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
    <attnum name="length margin" min="1" max="10" val="2"/>
    <attnum name="scale bump" val="0.337"/>
    <attnum name="scale bump outer" val="0.5055"/>
    <attnum name="scale brake" val="0.58"/>
    <attnum name="scale mu" val="1.01"/>
    <attnum name="qualy brake" val="0.84"/>
    <attnum name="qualy mu" val="1.05"/>
  </section>

</params>

