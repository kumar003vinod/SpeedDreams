<!DOCTYPE params SYSTEM "params.dtd">


<params name="trb1-cavallo-360rb">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="12" val="12.0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="17" val="11.5"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-3"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="3.896"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.468"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.818"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.429"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1.169"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="1"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" min="1" max="10" val="3.25"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition"  min="0.3" max="0.7" val="0.65"/>
    <attnum name="max pressure" unit="kPa" min="20000" max="100000" val="42500"/>
    <attnum name="brake corr lr" min="0.0" max="0.5" val="0.30"/>
    <attnum name="brake corr fr" min="0.0" max="0.5" val="0.14"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="0" max="5000" val="2500"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="1000" max="10000" val="2000"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.15"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="50" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="300"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="400"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="250"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="300"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="1000" max="10000" val="2000"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.15"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="50" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="300"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="400"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="250"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="300"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="1000" max="10000" val="2000"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="50" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="300"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="300"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="275"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="170"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="1000" max="10000" val="2000"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="1.5"/>
    <attnum name="packers" min="0" max="50" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="300"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="300"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="275"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="170"/>
  </section>

  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="100" val="-1"/>

    <attnum name="scale brake" min="0.5" max="2.5" val="0.85"/>
    <attnum name="scale mu" min="0.5" max="2.5" val="0.80"/>
    <attnum name="scale bump" min="0.0" max="2.0" val="0.4"/>
    <attnum name="base mode" min="0.97" max="0.999" val="0.997"/>
    <attnum name="base scale" min="1" max="1.05" val="1.0160"/>

    <attnum name="BC_P" min="0.0" max="30.0" val="3.0"/>
    <attnum name="BC_I" min="0.0" max="0.0" val="0.0"/>
    <attnum name="BC_D" min="0.0" max="100.0" val="20.0"/>
    <attnum name="BC_MAX" min="0.0" max="100.0" val="1.0"/>
    <attnum name="BC_MIN" min="-100.0" max="0.0" val="0.0"/>

    <attnum name="lookaheadfactor" min="0.01" max="0.5" val="0.05"/>

    <attnum name="qualy brake" min="0.5" max="2.5" val="0.95"/>
    <attnum name="qualy mu" min="0.5" max="2.5" val="1.0"/>

    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" val="0"/>
    <attnum name="reserve" min="0" max="20000" val="2000"/>
    <attnum name="start fuel" min="-1" max="100" val="-1"/>
    <attnum name="fuelper100km" min="1" max="100" val="70"/>
    <attnum name="avoid width" min="0" max="3" val="0.5"/>
    <attnum name="drift factor" min="1" max="10" val="1.5"/>
    <attnum name="scale avoid" min="1" max="20" val="10"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="scale mu rain" min="0" max="5" val="0.9"/>
    <attnum name="scale brake rain" min="0" max="1" val="0.9"/>
    <attnum name="scale steer" min="0.1" max="2" val="1"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
    <attnum name="length margin" min="1" max="10" val="2"/>
    <attnum name="scale bump outer" min="0.0" max="3.0" val="0.6"/>
    <attnum name="skill" min="-1" max="10" val="0"/>
  </section>

</params>

