<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Viper GTS-R" type="template">

	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="12" val="12"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="18" val="18"/>
	</section>
	
	<section name="Rear Left Suspension">
		<attnum name="bellcrank" min="0.1" max="5" val="1.3"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="bellcrank" min="0.1" max="5" val="1.3"/>
	</section>

	<section name="private">
		<attnum name="base speed" val="17.3"/>
		<attnum name="bump caution" val="0.0"/>

		<attnum name="0 bgn" val="1110"/>
		<attnum name="0 end" val="1114"/>
		<attnum name="0 speed limit" val="60"/>

		<attnum name="1 bgn" val="740"/>
		<attnum name="1 end" val="750"/>
		<attnum name="1 speed limit" val="35"/>

		<attnum name="2 bgn" val="751"/>
		<attnum name="2 end" val="850"/>
		<attnum name="2 bump caution" val="0.1"/>
	</section>

</params>














