<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="36gp-milano-12c36" type="template">

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area" unit="m2" val="0"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-10.0" max="0.0" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="10" val="2.0"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="10" val="1.37"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="10" val="1.14"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="10" val="0.80"/>
      </section>
    </section>
  </section>

    <section name="Rear Differential">
      <attstr name="type"  val="FREE"/>
      <attnum name="ratio" min="0" max="10" val="3.75"/>
    </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.01" max="0.99" val="0.65"/>
    <attnum name="max pressure" unit="kPa" min="100" max="100000" val="19000"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="100" val="0"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="100" val="5"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="50" max="300" val="130"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="-0.1"/>
    <attnum name="camber" unit="deg" val="1"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="50" max="300" val="130"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0.1"/>
    <attnum name="camber" unit="deg" val="1"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="50" max="300" val="120"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" val="0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="50" max="300" val="120"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" val="0"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="0" max="500" val="24"/>
    <attnum name="suspension course" unit="m" min="0.001" max="0.5" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0" max="100" val="5.4"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0" max="100" val="8.3"/>
    <attnum name="fast bump" unit="kN/m/s" min="0" max="50" val="1.8"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0" max="50" val="2.8"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="0" max="500" val="24"/>
    <attnum name="suspension course" unit="m" min="0.001" max="0.5" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0" max="100" val="5.4"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0" max="100" val="8.3"/>
    <attnum name="fast bump" unit="kN/m/s" min="0" max="50" val="1.8"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0" max="50" val="2.8"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="0" max="500" val="19"/>
    <attnum name="suspension course" unit="m" min="0.001" max="0.5" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0" max="100" val="4.5"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0" max="100" val="6.9"/>
    <attnum name="fast bump" unit="kN/m/s" min="0" max="50" val="1.5"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0" max="50" val="2.3"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="0" max="500" val="19"/>
    <attnum name="suspension course" unit="m" min="0.001" max="0.5" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0" max="100" val="4.5"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0" max="100" val="6.9"/>
    <attnum name="fast bump" unit="kN/m/s" min="0" max="50" val="1.5"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0" max="50" val="2.3"/>
  </section>

  <section name="simplix private">

    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="fuelper100km" min="1.0" max="200.0" val="85.0"/>

    <attnum name="accel out" min="0" max="1" val="1"/>
    <attnum name="accel delta" min="0" max="1" val="0.08"/>

    <attnum name="tcl slip" min="1.0" max="10.0" val="1.1"/>
    <attnum name="tcl factor" min="1.0" max="10.0" val="1.0"/>
    <attnum name="drift factor" min="1.0" max="10.0" val="1.0"/>
    <attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.15"/>

    <attnum name="brake limit" min="-20" max="0" val="-6.75"/>
    <attnum name="scale bump" min="0" max="2" val="0.07"/>
    <attnum name="scale bump outer" min="0" max="2" val="0.5"/>
    <attnum name="scale brake" min="0" max="2" val="0.75"/>
    <attnum name="scale mu" min="0" max="2" val="0.70"/>
    <attnum name="scale steer" min="0.1" max="2.0" val="1.0"/>
    <attnum name="qualy brake" min="0" max="2" val="1.1"/>
    <attnum name="qualy mu" min="0" max="2" val="0.82"/>
  </section>
</params>
