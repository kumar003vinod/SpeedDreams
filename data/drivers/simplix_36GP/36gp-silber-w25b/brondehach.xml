<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="36gp-silber-w25b" type="template">

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area" unit="m2" val="0"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" val="2.0"/>
      </section>
      <section name="2">
	<attnum name="ratio" val="1.37"/>
      </section>
      <section name="3">
	<attnum name="ratio" val="1.14"/>
      </section>
      <section name="4">
	<attnum name="ratio" val="0.8"/>
      </section>
    </section>
  </section>

  <section name="Rear Differential">
    <attstr name="type" in="FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" val="5.97"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" val="0.6"/>
    <attnum name="max pressure" unit="kPa" val="20000"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" val="0"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" val="5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" val="27"/>
    <attnum name="suspension course" unit="m" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" val="4.5"/>
    <attnum name="slow rebound" unit="kN/m/s" val="9"/>
    <attnum name="fast bump" unit="kN/m/s" val="1.5"/>
    <attnum name="fast rebound" unit="kN/m/s" val="3"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" val="27"/>
    <attnum name="suspension course" unit="m" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" val="4.5"/>
    <attnum name="slow rebound" unit="kN/m/s" val="9"/>
    <attnum name="fast bump" unit="kN/m/s" val="1.5"/>
    <attnum name="fast rebound" unit="kN/m/s" val="3"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" val="15"/>
    <attnum name="suspension course" unit="m" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" val="3.5"/>
    <attnum name="slow rebound" unit="kN/m/s" val="7.0"/>
    <attnum name="fast bump" unit="kN/m/s" val="1.0"/>
    <attnum name="fast rebound" unit="kN/m/s" val="1.8"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" val="15"/>
    <attnum name="suspension course" unit="m" val="0.10"/>
    <attnum name="bellcrank" val="1.0"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" val="3.5"/>
    <attnum name="slow rebound" unit="kN/m/s" val="7.0"/>
    <attnum name="fast bump" unit="kN/m/s" val="1.0"/>
    <attnum name="fast rebound" unit="kN/m/s" val="1.8"/>
  </section>

  <section name="simplix private">
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="fuelper100km" min="1.0" max="200.0" val="105.0"/>

    <attnum name="accel out" min="0" max="1" val="1"/>
    <attnum name="accel delta" min="0" max="1" val="0.08"/>

    <attnum name="tcl slip" min="1.0" max="10.0" val="1.1"/>
    <attnum name="tcl factor" min="1.0" max="10.0" val="1.0"/>
    <attnum name="drift factor" min="1.0" max="10.0" val="1.0"/>
    <attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.25"/>

    <attnum name="brake limit" val="-10.0"/>
    <attnum name="scale bump" val="0.05"/>
    <attnum name="scale bump outer" val="0.5"/>
    <attnum name="scale brake" min="0" max="1" val="0.90"/>
    <attnum name="scale mu" min="0" max="2" val="0.68"/>
    <attnum name="scale steer" min="0.1" max="2.0" val="1.01"/>
    <attnum name="qualy brake" min="0" max="2" val="0.96"/>
    <attnum name="qualy mu" min="0" max="2" val="0.71"/>
  </section>
</params>
