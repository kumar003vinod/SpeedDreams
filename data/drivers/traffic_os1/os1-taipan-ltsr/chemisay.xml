<!DOCTYPE params SYSTEM "params.dtd">


<params name="ls1-taipan-ltsr">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" val="12"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" val="18.8"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-2"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="3.13"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.04"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.5"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.21"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="0.92"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="0.82"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" val="3.86"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" val="0.7"/>
    <attnum name="max pressure" unit="kPa" val="45000"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" val="9.8"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" val="10.6"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" min="60" max="300" unit="mm" val="60"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="-0.2"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" min="60" max="300" unit="mm" val="60"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="-0.2"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" min="60" max="300" unit="mm" val="60"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" min="60" max="300" unit="mm" val="60"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" val="100.0"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.16"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="70" unit="kN/m/s" val="13"/>
    <attnum name="slow rebound" min="0" max="70" unit="kN/m/s" val="26"/>
    <attnum name="fast bump" min="0" max="70" unit="kN/m/s" val="4"/>
    <attnum name="fast rebound" min="0" max="70" unit="kN/m/s" val="9"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" val="100.0"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.16"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="70" unit="kN/m/s" val="13"/>
    <attnum name="slow rebound" min="0" max="70" unit="kN/m/s" val="26"/>
    <attnum name="fast bump" min="0" max="70" unit="kN/m/s" val="4"/>
    <attnum name="fast rebound" min="0" max="70" unit="kN/m/s" val="9"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" val="115"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.14"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="70" unit="kN/m/s" val="11"/>
    <attnum name="slow rebound" min="0" max="70" unit="kN/m/s" val="22"/>
    <attnum name="fast bump" min="0" max="70" unit="kN/m/s" val="4"/>
    <attnum name="fast rebound" min="0" max="70" unit="kN/m/s" val="7"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" val="115"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.14"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="70" unit="kN/m/s" val="11"/>
    <attnum name="slow rebound" min="0" max="70" unit="kN/m/s" val="22"/>
    <attnum name="fast bump" min="0" max="70" unit="kN/m/s" val="4"/>
    <attnum name="fast rebound" min="0" max="70" unit="kN/m/s" val="7"/>
  </section>

  <section name="simplix private">
    <attnum name="pit test stop" max="1" val="0"/>
    <attnum name="qualification" val="0"/>
    <attnum name="reserve" min="0" max="20000" unit="m" val="2000"/>
    <attnum name="start fuel" max="60" val="-1"/>
    <attnum name="fuelper100km" min="1" max="100" val="80"/>
    <attnum name="scale bump" val="0"/>
    <attnum name="scale bump outer" val="0"/>
    <attnum name="accel out" min="0" val="1"/>
    <attnum name="fly height" min="0.01" max="1" val="0.2"/>
    <attnum name="jump offset" min="0" max="0.1" val="0.04"/>
    <attnum name="tcl slip" max="10" val="1"/>
    <attnum name="tcl factor" min="1" max="10" val="1.4"/>
    <attnum name="lookaheadfactor" val="0.19"/>
    <attnum name="scale steer" min="0.1" max="2" val="1"/>
    <attnum name="brake limit" val="-15"/>
    <attnum name="scale brake" val="0.84"/>
    <attnum name="scale mu" val="0.88"/>
    <attnum name="scale brake rain" min="0" val="1"/>
    <attnum name="scale mu rain" min="0" val="1"/>
    <attnum name="qualy brake" val="1.04"/>
    <attnum name="qualy mu" val="0.93"/>
    <attnum name="length margin" min="1" max="10" val="3"/>
    <attnum name="avoid width" min="0" max="3" val="0.5"/>
    <attnum name="scale avoid" min="1" max="20" val="15"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="stay together" max="2000" val="10"/>
  </section>

</params>
