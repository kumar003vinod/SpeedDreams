<!DOCTYPE params SYSTEM "params.dtd">


<params name="ls2-bavaria-g3gtr">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="6" max="10" val="10"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="2" max="20" val="10"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-2"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="5"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="3.91"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="2.79"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.9"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1.4"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="1.05"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" min="1" max="10" val="3.35"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.64"/>
    <attnum name="max pressure" unit="kPa" min="100" max="23000" val="23000"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="10"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="18"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="-0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="-0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-4"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-4"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="63.4"/>
    <attnum name="suspension course" unit="m" min="0" max="0.15" val="0.15"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="9.3"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="14.3"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="2.1"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="4.8"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="63.4"/>
    <attnum name="suspension course" unit="m" min="0" max="0.15" val="0.15"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="9.3"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="14.3"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="2.1"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="4.8"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="53.4"/>
    <attnum name="suspension course" unit="m" min="0" max="0.15" val="0.15"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="7.7"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="11.8"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="2.6"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="3.9"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="53.4"/>
    <attnum name="suspension course" unit="m" min="0" max="0.15" val="0.15"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="7.7"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="11.8"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="2.6"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="3.9"/>
  </section>

  <section name="simplix private">
    <attnum name="initial fuel" min="-1" max="100" val="-1"/>
    <attnum name="scale brake" min="0" max="2" val="0.98"/>
    <attnum name="scale mu" min="0" max="5" val="0.8"/>
    <attnum name="scale bump" min="0" max="0.6" val="0.3"/>

    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" unit="m" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="80"/>
    <attnum name="accel out" min="0" max="1" val="1"/>
    <attnum name="base mode" min="0.5" max="1.5" val="0.98"/>
    <attnum name="clutch max" min="0.001" max="1" val="0.5"/>
    <attnum name="accel delta rain" min="0.0001" max="0.5" val="0.0002"/>
    <attnum name="fly height" min="0.01" max="1" val="0.2"/>
    <attnum name="jump offset" min="0" max="0.1" val="0.04"/>
    <attnum name="tcl slip" min="1" max="10" val="1"/>
    <attnum name="tcl factor" min="1" max="10" val="1"/>
    <attnum name="drift factor" min="1" max="10" val="2"/>
    <attnum name="lookaheadfactor" min="0" max="0.5" val="0.1"/>
    <attnum name="scale steer" min="0.1" max="2" val="0.8"/>
    <attnum name="brake limit" min="-9.9" max="-9.9" val="-9.9"/>
    <attnum name="scale brake rain" min="0" max="1" val="0.85"/>
    <attnum name="scale mu rain" min="0" max="1" val="0.85"/>
    <attnum name="scale bump outer" min="0.45" max="0.45" val="0.45"/>
    <attnum name="qualy brake" min="0" max="2" val="0.98"/>
    <attnum name="qualy mu" min="0" max="2" val="0.83"/>
    <attnum name="length margin" min="1" max="10" val="3"/>
    <attnum name="avoid width" min="0" max="3" val="0.5"/>
    <attnum name="scale avoid" min="1" max="20" val="20"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
  </section>

</params>
